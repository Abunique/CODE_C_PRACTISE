#include<stdio.h>

char *buff ="consistency is the key. time and energy are the most important wealth we have in hand";

typedef char word[20];

word letter[20];

void string_op()
{
int wordcount = 0, lettercount = 0;

while(*buff != '0')
{
	if(*buff == ' ')
	{
		wordcount++;
	}
	else
	{
		letter[wordcount][lettercount]= *buff;
		lettercount++;
	}
	buff++;
}

}


int main()
{
	printf("test");
	getchar();
	return 0;
}
