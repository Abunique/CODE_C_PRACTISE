#include<stdio.h>

char *buff ="consistency is the key. time and energy are the most important wealth we have in hand";

typedef char word[20];

word letter[20];

void string_op()
{
int wordcount = 0, lettercount = 0;

while(*buff != '0')
{
	if(*buff == ' ')
	{
		wordcount++;
	}
	else
	{
		letter[wordcount][lettercount]= *buff;
		lettercount++;
	}
	buff++;
}

}

void print_words()
{
	for(int i=0;i<wordcount;i++)
	{|
		printf("%s\n",word[i]);
	}
}


int main()
{
	string_op();

	getchar();
	return 0;
}
